{
	"name": "smtech/canvas-ics-sync",
	"description": "An LTI and service to sync Webcal/ICS feeds into and out of Canvas",
    "authors": [
		{
			"name": "Seth Battis",
			"email": "sethbattis@stmarksschool.org"
		}
